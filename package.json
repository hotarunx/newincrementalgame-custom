{
    "name": "newincrementalgame-custom",
    "version": "1.0.0",
    "description": "新しい放置ゲーム(1).fileを改造する。",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "lint": "eslint --ignore-path .gitignore \"./src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "npm run lint --fix",
        "format": "prettier --write --ignore-path .gitignore \"**/*.{css,scss,html,js,json,jsx,md,ts,tsx}\"",
        "test": "npm run lint"
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^5.12.1",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.4.0",
        "prettier": "^2.5.1",
        "typescript": "^4.5.5"
    }
}
